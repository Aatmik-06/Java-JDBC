<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.1" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.demo.controller.P1Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>P2Controller</servlet-name>
        <servlet-class>com.demo.controller.P2Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>P3Controller</servlet-name>
        <servlet-class>com.demo.controller.P3Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>P2Controller</servlet-name>
        <url-pattern>/P2Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>P3Controller</servlet-name>
        <url-pattern>/P3Controller</url-pattern>
    </servlet-mapping>
</web-app>
